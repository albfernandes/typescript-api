{
  "name": "stock-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "cd .. && husky install .husky",
    "build": "tsc -p .",
    "test": "jest tests",
    "test:system": "jest tests/system",
    "start:local": "npm run update-routes && concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "update-routes": "tsoa spec-and-routes",
    "test:system:watch": "jest tests/system --watch --coverage=false",
    "test:unit": "jest tests/unit",
    "test:unit:watch": "jest tests/unit --watch --coverage=false",
    "lint": "eslint . --ext ts",
    "test:integration": "jest tests/integration",
    "test:integration:watch": "jest tests/integration --watch --coverage=false",
    "pretest": "npm run build && npm run lint"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.32",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-spellcheck": "0.0.17",
    "faker": "^5.4.0",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsdef": "0.0.14",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "csv-parse": "^5.0.4",
    "express": "^4.17.1",
    "inversify": "^5.0.5",
    "reflect-metadata": "^0.1.13",
    "tsoa": "^3.5.0"
  }
}
